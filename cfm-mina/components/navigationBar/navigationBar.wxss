/* components/nav/nav.wxss */

.title {
    position: sticky;
    top: 0;
    width: 750rpx;
    height: 80rpx;
    z-index: 99999;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  .nav-back-block {
    display: flex;
    height: 32px;
    width: 86px;
    align-items: center;
    justify-content: center;
    position: absolute;
    left: 10px;
    bottom: 15rpx;
    border-radius: 32px;
    background-color: rgba(255, 255, 255, 0.65);
    border: 1rpx solid rgba(214, 212, 211, 0.5);
    box-sizing: border-box;
    z-index: 10000;
  }
  
  .nav-back-icon {
    height: 16rpx;
    width: 16rpx;
    border: 6rpx solid black;
    border-right-color: transparent;
    border-bottom-color: transparent;
    transform: rotate(-45deg);
  }
  
  .nav-back-text {
    font-size: 16px;
  }
  
  .header{
    /* height: 100%; */
    padding-top: 55rpx;
    position: fixed;
    width: 100%;
    top: 0;
    background-color: #ffffff;
    z-index: 2;
   }
   .picker-height{
     width:100%;
     position: relative;
     z-index: 1023;
     background: #fff;
     margin-top: 2rpx;
   }
   .picker-height>view:first-child .area{
     height:60rpx;
     display: flex;
     color: #050FDA;
     align-items: flex-end;
     padding-left: 30rpx;
   }
   .area>text {
     margin-left: 6rpx;
   }

