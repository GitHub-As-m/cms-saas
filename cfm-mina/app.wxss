/*底部按钮的公共样式  */
.submit-bottom{
  position: fixed;
  left: 60rpx;
  bottom: 40rpx;
  background: #323DDB;
  color: #fff;
  width: 84%;
  line-height: 88rpx;
  text-align: center;
  font-size: 30rpx;
  z-index: 999;
  border-radius: 44rpx;
}
/*底部按钮的公共样式  */

/* steps start */
.stepsProcess{
  padding: 30rpx 24rpx 0 24rpx;
  position: relative;
  background: #fff;
  height: 360rpx;
}
.pre{
  height:auto;
}
.status3ProcessLine{
  width: 1rpx;
  height: 240rpx;
  background: #eee;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.stepsProcessCon{
  position: absolute;
  left: 24rpx;
  top: 30rpx;
}
.pre .stepsProcessCon{
  position: relative;
  left: 0;
  top:0;
  z-index: 0;
}
.pre .processItem{
  position: relative;
}
.preLine .processItem:last-child .processItemCon view:last-child{
  color:#83ABE2;
} 
.prev .processItem:last-child .lined{
  display: none;
}
.lined{
  width: 1rpx;
  height: 120rpx;
  background: #eee;
  position: absolute;
  left:10rpx;
  top:22rpx;
}
.processItem{
  height: 120rpx;
}
.processItem:last-child .lined{
  background:  transparent;
}
.processItem>view:first-child{
  width: 16rpx;
  height: 16rpx;
  border: 5rpx solid #fff;
  margin-right: 20rpx;
  float: left;
  background: #eeeeee;
  border-radius: 50%;
}
.processItem>view.done{
  background: #1CC296;
  border-color: #1CC296;
}
.processItemCon{
  float: left;
}
.processItemCon>view:first-child{
  line-height: 30rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: #18191A;
}
.processItemCon>view:last-child{
  line-height: 26rpx;
  font-size: 26rpx;
  color: #757B82;
  font-weight: 400;
  margin-bottom: 20rpx;
}
.status3Content>view:first-child,.status4Content>view:first-child{
  color: #fff;
  font-size: 30rpx;
  line-height: 30rpx;
  margin-bottom: 20rpx;
}
.status3Content>view:nth-child(2),.status4Content>view:nth-child(2){
  color: #fff;
  line-height: 30rpx;
  font-size: 24rpx;
}
.status3Content .countDown1,.status4Content .countDown1{
  font-size: 40rpx;
  margin-right: 20rpx;
}
.status4Content>view:last-child{
  margin-top: 35rpx;
  height: 64rpx;
}
.status3Content>view:nth-child(3){
  margin-top: 20rpx;
}
/* steps end */
