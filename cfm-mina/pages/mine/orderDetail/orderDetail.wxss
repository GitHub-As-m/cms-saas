.margin-bottom {
  margin-bottom: 16rpx;
}
.grey {
  color: #8D8E92;
}
.order-detail{
  padding-bottom: 20rpx;
}
.od-header{
  height: 180rpx;
  position: relative;
}
.od-header>image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
}
.status1Detail{
  display: flex;
  position: absolute;
  top: 44rpx;
  left: 32rpx;
  color: #fff;
}
.status1Detail>text {
  display: inline-block;
  font-size: 40rpx;
}
.status1Content{
  margin-left: 32rpx;
}
.status1Content>view:nth-child(1) {
  font-size: 40rpx;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  font-weight: 500;
}
.status1Content>view:nth-child(2) {
  font-size: 28rpx;
  font-weight: 500;
}
.od-info-address {
  background-color: #ffffff;
}
.od-info{
  padding: 24rpx 0 24rpx 28rpx;
  border-bottom: 1px solid #F0F0F0;
}
.od-psinfo{
  padding-bottom: 10rpx;
}
.od-psinfo>text{
  color:#2C3038;
  font-size: 28rpx;
  font-weight: 400;
}
.od-psinfo>text:nth-child(1) {
  margin-right: 52rpx;
}
.od-address{
  color: #8D8E92;
  font-size: 26rpx;
}
.od-time{
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #2C3038;
  font-size: 28rpx;
  font-weight: 400;
  padding: 24rpx 0 24rpx 26rpx;
}
.od-time .modify{
  display: inline-block;
  width: 80rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  color: #1CC296;
  font-size: 26rpx;
  font-weight: 500;
  border-radius: 28rpx;
  border: 1px solid #1CC296;
  margin-right: 28rpx;
}
.od-info-goods{
  background-color: #ffffff;
}
.shop-name{
  color: #2C3038;
  font-size: 28rpx;
  font-weight: 500;
  padding: 20rpx 0 20rpx 26rpx;
  border-bottom: 1px solid #F0F0F0;
  display: flex;
  justify-content: space-between;
}
.shop-name>text {
  margin-right: 12rpx;
  vertical-align: bottom;
}
.shop-name .order-number{
  font-size: 22rpx;
}
.shop-item-status1Content{
  font-size: 26rpx;
  padding-right: 26rpx;
}
.shop-item-status1Content .iconfont{
  font-size: 24rpx;
}
.shop-item-status1Content >view {
  color: #1CC296;
}
.shop-item-status1Content >view text{
  color: #C6C6C6;
}
.goods-item{
  display: flex;
  flex-wrap: wrap;
  border-bottom: 1px solid #F0F0F0;
  padding: 20rpx 28rpx 20rpx 0;
  margin-left: 28rpx;
}
.goods-item>image{
  width: 70rpx;
  height: 70rpx;
  border: 1px solid #F2F2F2;
  margin-right: 16rpx;
}
.item-info{
  min-width: 200rpx;
}
.item-info>view:nth-child(1) {
  color: #2C3038;
  font-size: 26rpx;
  margin-bottom: 8rpx;
}
/* .item-info>view:nth-child(2) {
  color: #8D8E92;
  font-size: 24rpx;
} */
.infoPrice {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #757B82;
  line-height: 36rpx;
}
.oldPrice {
  text-decoration: line-through;
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #757B82;
  line-height: 36rpx;
  display: inline-block;
}
.item-info text {
  margin-left: 10rpx;
}
.info-right{
  flex: 1;
  display: flex;
  justify-content: flex-end;
}
.number{
  color: #8D8E92;
  font-size: 24rpx;
}
.total-price{
  color: #2C3038;
  font-size: 24rpx;
  margin-left: 60rpx;
}
.remarks{
  margin-left: 28rpx;
  padding: 10px 0;
  font-weight: 400;
  border-bottom: 1px solid #F0F0F0;
}
.remarks>text:nth-child(1) {
  color: #2C3038;
  font-size: 26rpx;
}
.remarks>text:nth-child(2) {
  color: #8D8E92;
  font-size: 26rpx;
}
.subtotal{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  color: #2C3038;
  font-size: 26rpx;
  padding: 16rpx 28rpx 16rpx 0;
}
.subtotal>text:nth-child(2) {
  color: #FB3640;
  font-size: 32rpx;
  margin-left: 10rpx;
}
.amount-of-money{
  background-color: #ffffff;
  padding: 26rpx 28rpx;
}
.amount-of-money>view {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
}
.amount-of-money>view>text {
  color: #040F0F;
  font-size: 28rpx;
  font-weight: 400;
}
.paid-in{
  color: #FB3640!important;
  font-weight: 500!important;
}
.other-info{
  margin-bottom: 118rpx;
  background-color: #FFFFFF;
  padding: 26rpx 28rpx;
}
.other-info>view{
  font-size: 28rpx;
  color: #040F0F;
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
}
.other-info>view>text {
  font-size: 28rpx;
  font-weight: 400;
}
.content-footer {
  background-color: #ffffff;
  display: flex;
  bottom: 0;
  width: 100%;
  justify-content: flex-end;
}
.content-footer button {
  width: 152rpx;
  height: 60rpx;
  line-height: 56rpx;
  text-align: center;
  color: #2C3038;
  border-radius: 30rpx;
  border: 1rpx solid #ABACAF!important;
  margin: 18rpx 28rpx 18rpx 0;
  padding: 0;
  font-size: 28rpx;
}
.footer{
  background-color: #ffffff;
  display: flex;
  position: fixed;
  bottom: 0;
  width: 100%;
  justify-content: flex-end;
}
.footer button {
  width: 152rpx;
  height: 60rpx;
  line-height: 56rpx;
  text-align: center;
  color: #2C3038;
  border-radius: 30rpx;
  border: 1rpx solid #ABACAF!important;
  margin: 18rpx 28rpx 18rpx 0;
  padding: 0;
  font-size: 28rpx;
}
.footer .to-pay{
  width: 192rpx;
  font-weight: 500;
  color: #1CC296!important;
  border: 1px solid #A4E7D5!important;
}
/* van-step-s */
.processItemCon {
  display: flex;
  justify-content: space-between;
  width: 92%;
}
.processItemCon>view:nth-child(1) {
  color:#2C3038;
  font-size: 30rpx;
  font-weight: 500;
}
.processItemCon>view:nth-child(2) {
  color:#8D8E92;
  font-size: 30rpx;
  font-weight: 400;
}
/* van-step-e */
.reason-list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 28rpx;
  font-size: 30rpx;
  color: #444547;
  height: 80rpx;
  line-height: height 80px;
}
.icon-cms_gou-227{
  color: #1CC296;
}
.reason-list .active{
  color:#1CC296;
}
.cancle-btn{
  width: 90%;
  margin: 30rpx auto 20rpx;
  background-color: #1CC296!important;
  border: none!important;
  color: #ffffff!important;
  border-radius: 44rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.stepsProcess{
  max-height: 800rpx;
  overflow-y: scroll; 
}
.goods-content .tips {
  color: #1CC296;
  font-size: 24rpx;
}
#map {
  width: 100%;
  height: 474rpx;
}
.customerMap {
  width: 148rpx;
  height: 44rpx;
  /* background: linear-gradient(226deg, #45DE60 0%, #1CC296 100%); */
  background: #1CC296;
  border-radius: 30rpx;
  color: #fff;
  line-height: 24rpx;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 4rpx;
}
.customerMap>cover-view {
}
.storeMap {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}
.icon-kfd_dianhua{
  color: #1CC296;
}