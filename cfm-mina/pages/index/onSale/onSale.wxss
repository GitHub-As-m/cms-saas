.onSale {
  width: 100%;
  background: #F5F5F5;
}
.onSaleTime {
  position: relative;
  width: 100%;
}
.timeTop {
  height: 320rpx;
  background: linear-gradient(90deg, #FD5144 0%, #FD4850 100%);
}
.onSaleNavbar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  top: 118rpx;
  padding: 0 32rpx 0 0;
  color: #fff;
}
/* .onSaleNavbar>view {
  flex:1;
} */
.back {
  width: 100rpx;
  padding-left: 40rpx;
}
.onSaleNavbar .back {
  display: inline-block;
}
.back>.iconfont {
  font-size: 40rpx;
}
.navbarText {
  /* display: inline-block; */
  text-align: center;
  margin-left: 180rpx;
}
.time {
  position: absolute;
  left: 3%;
  top: 200rpx;
  width: 710rpx;
  height: 164rpx;
  background: #FFFFFF;
  box-shadow: 0 4rpx 10rpx 0 rgba(221,221,221,1);
  border-radius: 16rpx;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.time>view:nth-child(2) {
  height: 34rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #FB3640;
  line-height: 34rpx;
  margin-top: 26rpx;
}
.timeInfo {
  height: 40rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #2C3038;
  line-height: 40rpx;
}
.timeShow {
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #FFFFFF;
  line-height: 20px;
  background-color: #FB3640;
  border-radius: 4rpx;
  text-align: center;
  padding-top: 4rpx;
  box-sizing: border-box;
}
.timeButtom {
  height: 90rpx;
}
.onSaleGoods {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.saleGoods {
  width: 92%;
  border-radius: 16rpx;
  display: flex;
  background-color: #fff;
  height: 220rpx;
  box-sizing: border-box;
  padding: 18rpx;
  margin-bottom: 20rpx;
}
.imUrl {
  height: 178rpx;
  width: 178rpx;
  box-sizing: border-box;
  position: relative;
}
.imUrl>image {
  height: 178rpx;
  width: 178rpx;
  border: 1px solid rgba(151, 151, 151, 0.2);
  box-sizing: border-box;
  border-radius: 6rpx;
}
.lineStock {
  position: absolute;
  bottom: 0;
  height: 30rpx;
  width: 100%;
  left:0;
  background: rgba(0, 0, 0, 0.5);
  font-size: 20rpx;
  color:#fff;
  line-height: 30rpx;
  text-align: center;
  border-bottom-left-radius: 6rpx;
  border-bottom-right-radius: 6rpx;
}
.goodsCover{
  width:178rpx;
  height:178rpx;
  line-height: 178rpx;
  background: rgba(255, 255, 255, 0.28);
  top: 0;
  left: 0;
  border: none;
  position: absolute;
}
.goodsCover .cover {
  display: flex;
  height: 46rpx;
  width: 120rpx;
  border-radius: 43rpx;
  line-height: 46rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  left: 25rpx;
  top: 62rpx;
  display: flex;
  justify-content: center;
  font-size: 26rpx;
}
.saleGoodsInfo {
  display: flex;
  flex-direction: column;
  margin-left: 26rpx;
  width: 70%;
}
.infoName {
  height: 42rpx;
  font-size: 30rpx;
  font-weight: 600;
  color: #040F0F;
  line-height: 42rpx;
}
.infoName>view {
  display: inline-block;
}
.shopName {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #8D8E92;
  line-height: 36rpx;
  padding-top: 6rpx;
}
.infoButtom {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 68rpx;
  margin-top: 32rpx;
}
.infoPrice {
  height: 44rpx;
  font-size: 32rpx;
  font-weight: 700;
  color: #FB3640;
  line-height: 80rpx;
}
.oldPrice {
  text-decoration: line-through;
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #757B82;
  line-height: 36rpx;
  display: inline-block;
}
.button {
  height: 64rpx;
  width: 136rpx;
  border-radius: 8rpx;
  background-color: #D9D9D9;
  color: #fff;
  text-align: center;
  font-weight: 500;
  padding: 9rpx;
  box-sizing: border-box;
}
.gray {
  background-color: #FB3640 ;
}
.flgray {
  background-color: #D9D9D9;
}
.loadAll {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #8D8E92;
  height: 42rpx;
}