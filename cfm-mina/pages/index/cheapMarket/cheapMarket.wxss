.marketBackgroud {
  width: 100%;
  height: 300rpx;
}
.marketBackgroud>image {
  width: 100%;
  height: 100%;
}
.market {
  background: linear-gradient(360deg, #CDEF9A 0%, #9FDB61 100%);
  min-height: 80%;
}
.marketTitle {
  color: #fff;
  text-align: center;
  height: 116rpx;
  line-height: 116rpx;
  font-size: 42rpx;
  font-weight: 600;
}
.marketGoods{
  width: 100%;
  /* overflow: scroll; */
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 16rpx;
  min-height: 100%;
}
.GoodsItem{
  height: auto;
  width: 47.5%;
  background: #fff;
  display: inline-block;
  margin: 6rpx 0 10rpx 6rpx;
}
.GoodsItem:nth-child(odd) {
  margin-left: 0rpx;
}
.imUrl {
  height: 354rpx;
  width: 100%;
  /* border-radius: 4rpx; */
  box-sizing: border-box;
  position: relative;
}
.imUrl>image {
  height: 354rpx;
  width: 100%;
  /* border: 1px solid rgba(151, 151, 151, 0.2); */
  box-sizing: border-box;
  /* border-radius: 6rpx; */
}
.lineStock {
  position: absolute;
  bottom: 0;
  height: 46rpx;
  width: 100%;
  left:0;
  background: rgba(0, 0, 0, 0.5);
  font-size: 24rpx;
  color:#fff;
  line-height: 46rpx;
  text-align: center;
  /* border-bottom-left-radius: 6rpx;
  border-bottom-right-radius: 6rpx; */
}
.goodsCover{
  height: 354rpx;
  width: 100%;
  line-height: 178rpx;
  background: rgba(255, 255, 255, 0.28);
  top: 0;
  left: 0;
  border: none;
  position: absolute;
}
.goodsCover .cover {
  display: flex;
  height: 46rpx;
  width: 120rpx;
  border-radius: 43rpx;
  line-height: 46rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.5);
  position: absolute;
  left: 113rpx;
  top: 140rpx;
  display: flex;
  justify-content: center;
  font-size: 26rpx;
}
.GoodsList_button {
  padding: 22rpx;
}
.title{
  font-size: 30rpx;
  color: #040F0F;
  display: inline;
  width: 100%;
  margin-bottom: 8rpx;
  font-weight: bolder;
  display: inline-block;
  white-space: nowrap; 
  overflow: hidden;
  text-overflow:ellipsis;
}
.specification {
  font-size: 30rpx;
  color: #040F0F;
  display: inline;
  width: 100%;
  margin-bottom: 8rpx;
  font-weight: bolder;
}
.store{
  font-size: 28rpx;
  color: #8D8E92;
  display: inline-block;
  width: 100%;
  margin-bottom: 8rpx;
  margin-left: -10rpx;
}
.foter{
  height: auto;
  display: flex;
  justify-content: space-between;
  margin-top: 12rpx;
}
.price{
  color: #FB3640;
  font-size: 30rpx;
  font-weight: bolder;
}
.infoPrice {
  height: 44rpx;
  font-size: 32rpx;
  font-weight: 700;
  color: #FB3640;
  line-height: 80rpx;
}
.oldPrice {
  text-decoration: line-through;
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #757B82;
  line-height: 36rpx;
  display: inline-block;
}
.addcar{
  height: 56rpx;
  width: 56rpx;
  border-radius: 50%;
  background-color: #1CC296;
  position: relative;
}
.addcar .iconfont {
  color: #fff;
  position: absolute;
  left: 11rpx;
  top: 12rpx;
}
.placeholder {
  background-color: #fff;
}
.loadAll {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #8D8E92;
  height: 42rpx;
  padding-bottom: 20rpx;
}