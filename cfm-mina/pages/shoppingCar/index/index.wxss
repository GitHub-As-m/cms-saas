  float: right;
}
.controlCenter text{
  color: #FB3640;
  font-size: 32rpx;
  font-weight:bold;
}
.controlRight{
  float: right;
  width: 240rpx;
  background: #1CC296;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
  border-radius: 48rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 9rpx auto;
  margin-left: 40rpx;
}

