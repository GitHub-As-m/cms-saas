.bindPhone{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .7);
  z-index: 1024;
}
.bindBox{
  width: 702rpx;
  height: 612rpx;
  border-radius: 30rpx;
  background: white;
  position: absolute;
  left: 24rpx;
  top: 50%;
  margin-top: -306rpx;
  box-sizing: border-box;
  padding: 60rpx 46rpx 0rpx;
}
.bindBox>text{
  font-size: 28rpx;
  color: #333;
  margin-bottom: 53rpx;
  display: block;
  padding: 0 10rpx;
}
.input,.submit{
  width: 610rpx;
  height: 88rpx;
  background: #f8f9fb;
  border-radius: 44rpx;
  margin-bottom: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 50rpx;
  padding-right: 40rpx; 
}
.input>input{
  font-size: 30rpx;
  color: #333;
  width: 360rpx;
}
.input>text{
  font-size: 28rpx;
  color: #83ABE2;
  text-align: center;
  width: 140rpx;
  height: 40rpx;
}
.submit{
  margin-top: 70rpx;
  margin-bottom: 0;
  justify-content: center;
  background: #83ABE2;
  color: #fff;
  font-size: 38rpx;
}
.bindPhoneErrTip{
  position: absolute;
  left: 0;
  bottom: 170rpx;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #ff7674;
}
.tip{
  line-height: 60rpx;
  text-align: center;
  background: #fef7da;
  font-size: 26rpx;
  color: #fabe00;
}
.list>view:last-child{
  height: 98rpx;
}
.listItem{
  background: #fff;
  width: 100%;
  margin: 0 auto;
  margin-bottom: 20rpx;
  margin-top: 10px;
  border-radius: 12rpx;
  box-shadow: 0 1px 6px 0 #E5E5E5;
}
.full-subtraction{
  display:flex;
  justify-content:space-start;
  align-items:center;
  margin-top: 10rpx;
}
.full-subtraction image{
 width: 56rpx;
 height: 32rpx;
 margin-left: 80rpx;
 margin-right: 20rpx;
}
.shopBox .s-coupon{
  display: flex;
  padding-left:78rpx;
  padding-bottom: 20rpx;
}
.shopBox .s-coupon text{
  display: block;
}
.shop,.shopName,.goodsItem,.itemContent{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.shop{
  line-height: 70rpx;
  height: 70rpx;
  padding: 10rpx;
  border-bottom: 1rpx #f2f2f2 solid;
}
.goodsItem>view:first-child,.select,.shop>view:first-child{
  position: relative;
}
.goodsItem>view:first-child{
  height:144rpx;
}
.goodsItem>view:first-child .select{
  margin-left:24rpx;
  margin-top:54rpx;
}
.shop>view:first-child{
  height:70rpx;
}
.shop>view:first-child .select{
  margin-left:12rpx;
  margin-top:17rpx; 
}
.select{
  margin-right: 20rpx;
}
.select>view{
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 1rpx solid #979797;
  box-sizing: border-box;
  /* margin-right: 20rpx; */
}
.selectCover{
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  position: absolute;
  top: 54rpx;
  left: 24rpx;
  background: #ccc;
}
.select>image{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.shopName{
  width: 540rpx;
  color: #2C3038;
  font-size: 30rpx;
}
.shopName>text{
  max-width: 485rpx;
   overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; 
}
.shopName>view{
  margin-left: 20rpx;
  font-size: 24rpx;
  padding-top: 4rpx;
}
.editBtn{
  color: #666;
  font-size: 28rpx;
  width: 110rpx;
  text-align: right;
}
.finishBtn{
  color: #ff7674;
}
.goodsList{
  padding: 20rpx 24rpx 20rpx 0rpx;
}
.goodsItem{
  height: 144rpx;
  margin-bottom: 40rpx;
}
.itemContent{
  width: 180rpx;
}
.itemImg{
  position: relative;
}
.itemCover{
  position: absolute;
  left: 2rpx;
  top: 2rpx;
  width: 145rpx;
  height: 145rpx;
  background: rgba(0, 0, 0, .6);
  opacity: 0.3;
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  line-height: 114rpx;
}
.itemButton{
  position: absolute;
  left: 2rpx;
  bottom: 2rpx;
  width: 145rpx;
  height: 28rpx;
  line-height: 28rpx;
  background: rgba(0, 0, 0, 0.5);
  color: #ffffff;
  font-size: 18rpx;
  text-align: center;
}
.itemImg>image{
  width: 144rpx;
  height: 144rpx;
  vertical-align: top;
  margin-right: 20rpx;
  border-radius: 6px;
  border: 1px solid #EDEDED;
}
.itemImg .distribution-Label{
  position: absolute;
  z-index: 2;
  left:0;
  top:0;
  background-image: linear-gradient(-90deg, #FD6674 0%, #FB3640 100%);
  padding:0 8rpx;
  height:30rpx;
  line-height: 30rpx;
  color:#fff;
  font-size: 20rpx;
  border-bottom-right-radius: 6rpx;
  border-top-left-radius: 6rpx;
}
.itemKind{
  height: 60rpx;
}
.itemKind>text{
  width: 260rpx;
  display: block;  
  line-height: 16rpx;
}
.itemKind>view{
  height:34rpx;
  width:303rpx;
  /* margin-bottom: 40rpx; */
  width: 100%;
  white-space: nowrap;/*不换行*/
  text-overflow:ellipsis;/*超出部分文字以...显示*/
  overflow: hidden;
}
.itemKind>view>.name{
  color: #333;
  line-height: 34rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-right: 20rpx;
  display: inline-block;
  float: left;
}
.itemKind>view>.specification{
  color: #333;
  line-height: 30rpx;
  height: 40rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.itemKind>view>.pre{
  display: block;
  height:32rpx;
  width:32rpx;
  font-size: 20rpx;
  color:#fff;
  text-align: center;
  line-height: 32rpx;
  background-image:linear-gradient(-269deg, #27B06E 0%, #2DC46A 100%);
  float: left;
  margin-left:10rpx;
}
.itemKind>view>.pre2{
  width:54rpx;
  background-image:linear-gradient(-269deg, #C39D63 0%, #D8AD6A 100%);
}
.itemKind>text:nth-child(2){
  color: #666;
  font-size: 22rpx;
  line-height: 22rpx;
  margin-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.itemKind>text:last-child{
}
.itemKind>text:last-child{
  padding-top: 20rpx;
}
.changeShopCar .foodPrice {
